{
    "name": "tokimikichika/factory-pattern",
    "description": "Практический пример паттерна Фабричный метод (парсеры JSON/XML/CSV)",
    "type": "library",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Tokimikichika\\FactoryPattern\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "cs-fix": "php-cs-fixer fix",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-sniff": "phpcs",
        "cs-sniff-fix": "phpcbf",
        "quality": [
            "@cs-check",
            "@cs-sniff",
            "@test"
        ]
    }
}



